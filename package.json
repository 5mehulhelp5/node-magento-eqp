{
	"name": "@netresearch/node-magento-eqp",
	"version": "1.0.0",
	"description": "Simple API wrapper around the Magento Marketplace EQP API",
	"main": "dist/index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/netresearch/node-magento-eqp.git"
	},
